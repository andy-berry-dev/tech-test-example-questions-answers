{
    "name": "questions-answers-frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@apollo/client": "~3.9.9",
        "@chakra-ui/react": "~2.8.2",
        "@emotion/react": "~11.11.4",
        "@emotion/styled": "~11.11.0",
        "@graphql-codegen/cli": "~5.0.2",
        "@graphql-codegen/typescript": "~4.0.6",
        "@graphql-codegen/typescript-operations": "~4.2.0",
        "@testing-library/jest-dom": "~5.17.0",
        "@testing-library/react": "~13.4.0",
        "@testing-library/user-event": "~13.5.0",
        "@types/jest": "~27.5.2",
        "@types/node": "~16.18.91",
        "@types/react": "~18.2.73",
        "@types/react-dom": "~18.2.23",
        "@typescript-eslint/eslint-plugin": "~7.4.0",
        "@typescript-eslint/parser": "~7.4.0",
        "concurrently": "~8.2.2",
        "eslint-config-airbnb": "~19.0.4",
        "eslint-config-airbnb-typescript": "~18.0.0",
        "eslint-config-prettier": "~9.1.0",
        "eslint-plugin-import": "~2.29.1",
        "eslint-plugin-jsx-a11y": "~6.8.0",
        "eslint-plugin-prettier": "~5.1.3",
        "eslint-plugin-react": "~7.34.1",
        "eslint-plugin-react-hooks": "~4.6.0",
        "framer-motion": "~11.0.24",
        "graphql": "~16.8.1",
        "prettier": "~3.2.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-loader-spinner": "~6.1.6",
        "react-scripts": "5.0.1",
        "typescript": "~4.9.5",
        "web-vitals": "~2.1.4"
    },
    "scripts": {
        "start": "concurrently  --names \"Client,TypeGen\" -c \"bgMagenta,bgBlue\" \"npm run watch:react\" \"npm run watch:typegen\"",
        "watch:react": "react-scripts start",
        "watch:typegen": "npm run typegen -- --watch '../backend/**/*.graphql'",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "lint": "eslint .",
        "eject": "react-scripts eject",
        "typegen": "graphql-codegen --config codegen.yml"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
